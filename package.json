{
  "name": "js-craftcamp-website",
  "version": "2.0.0",
  "description": "Website for the JSCC (jscraftcamp.org)",
  "engines": {
    "node": ">= 10.0.0"
  },
  "dependencies": {
    "gatsby": "^2.13.25",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-sharp": "^2.2.7",
    "gatsby-source-filesystem": "2.1.5",
    "gatsby-transformer-json": "2.2.2",
    "gatsby-transformer-sharp": "^2.2.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "hamjest": "^3.2.3",
    "mocha": "^6.1.4",
    "recursive-readdir-sync": "^1.0.6",
    "prettier": "^1.18.2"
  },
  "scripts": {
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "prebuild": "rm -rfv dist ; mkdir -p ./public",
    "build": "npm run build:participants.json ; gatsby build ; cp CNAME public/",
    "build:participants.json": "node scripts/concat-json.js ./participants public/participants.json; cp ./public/participants.json ./src/",
    "test": "mocha test"
  },
  "author": "Robert Hostlowsky",
  "license": "MIT",
  "repository": "https://github.com/jscraftcamp/website.git"
}
